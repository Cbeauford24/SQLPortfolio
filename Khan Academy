SELECT * FROM restarurant;

SELECT * FROM restarurant
WHERE city = 'New York City';

SELECT * FROM restarurant
WHERE rating <3;

SELECT * FROM restarurant
ORDER BY name;

SELECT * FROM restarurant
ORDER BY rating desc;

ALTER table restarurant  RENAME TO restaurant;
